<h1>新建文件</h1>

<%= form_for :article, url: articles_path do |f| %>
  <p>
    <strong>标题</strong><br>
    <%= f.text_area :title %>
  </p>
   <p>
    <strong>类型</strong><br>
    <%= f.text_area :category %>
  </p>
  
  <p>
    <strong>内容</strong><br>
    <%= f.text_area :text %>
  </p>
  
  <p>
    <%= f.submit "确认" %>
    <%= link_to '返回', articles_path %>
  </p>
<% end %>
